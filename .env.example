# Google AI Configuration (Optional - only needed if using LangGraph)
MODEL=gemini-2.0-flash
GOOGLE_API_KEY=your_google_api_key_here

# ServiceNow A2A Agent Configuration
A2A_CLIENT_BASE_URL=https://your-instance.service-now.com/
A2A_CLIENT_AGENT_CARD_PATH=/api/sn_aia/a2a/id/
A2A_CLIENT_AGENT_ID=your_agent_sys_id_here
A2A_CLIENT_AGENT_CARD_WELL_KNOWN_PATH=/well_known/agent_json

# OAuth Credentials for ServiceNow
A2A_CLIENT_ID=your_oauth_client_id_here
A2A_CLIENT_SECRET=your_oauth_client_secret_here
#  use this if it exists. If it does not work then get a new one using refresh token
A2A_CLIENT_AUTH_TOKEN=your_legacy_token_here
A2A_CLIENT_REFRESH_TOKEN=your_refresh_token_here

